<!DOCTYPE html>
<html>

<head>
  <%= render 'layouts/head' %>
  <%= render 'layouts/shim' %>
</head>

<body>
  <div class="main-wrapper">
    <%= render partial: 'layouts/header', locals: { product: @product , taxonomy: @taxonomy}%>
    <%= yield %>
    <%= render 'layouts/footer' %>
    <%= render 'layouts/copyright' %>
  </div>
  <%= render 'layouts/modal' %>
  <%= javascript_include_tag 'application' %>
</body>

</html>
